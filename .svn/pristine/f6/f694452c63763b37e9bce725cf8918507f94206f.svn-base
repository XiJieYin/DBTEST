package com.gx.hz.pojo;

import java.util.Date;

public class InfoTask {
    private Integer id;

    private Integer userId;

    private Short taskType;

    private String orgCode;

    private Date createTime;

    private Integer assignsPerson;
    
    //非数据库字段
    private Organization org;

    private String assignsPersonName;

    private String recorder;
    
    private String assigner;
    
   

	public String getRecorder() {
		return recorder;
	}

	public void setRecorder(String recorder) {
		this.recorder = recorder;
	}

	public String getAssigner() {
		return assigner;
	}

	public void setAssigner(String assigner) {
		this.assigner = assigner;
	}

	public String getAssignsPersonName() {
		return assignsPersonName;
	}

	public void setAssignsPersonName(String assignsPersonName) {
		this.assignsPersonName = assignsPersonName;
	}

	public Organization getOrg() {
		return org;
	}

	public void setOrg(Organization org) {
		this.org = org;
	}

	public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getUserId() {
        return userId;
    }

    public void setUserId(Integer userId) {
        this.userId = userId;
    }

    public Short getTaskType() {
        return taskType;
    }

    public void setTaskType(Short taskType) {
        this.taskType = taskType;
    }

    public String getOrgCode() {
        return orgCode;
    }

    public void setOrgCode(String orgCode) {
        this.orgCode = orgCode == null ? null : orgCode.trim();
    }

    public Date getCreateTime() {
        return createTime;
    }

    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }

    public Integer getAssignsPerson() {
        return assignsPerson;
    }

    public void setAssignsPerson(Integer assignsPerson) {
        this.assignsPerson = assignsPerson;
    }

	@Override
	public String toString() {
		return "InfoTask [id=" + id + ", userId=" + userId + ", taskType="
				+ taskType + ", orgCode=" + orgCode + ", createTime="
				+ createTime + ", assignsPerson=" + assignsPerson + ", org="
				+ org + ", assignsPersonName=" + assignsPersonName
				+ ", recorder=" + recorder + ", assigner=" + assigner + "]";
	}
    
    
}